#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/paulshapiro/Downloads/agent_workspace/antisemitic-hate-crime-tracker-portable/website-source/node_modules/.pnpm/pnpm@10.12.1/node_modules/pnpm/bin/node_modules:/Users/paulshapiro/Downloads/agent_workspace/antisemitic-hate-crime-tracker-portable/website-source/node_modules/.pnpm/pnpm@10.12.1/node_modules/pnpm/node_modules:/Users/paulshapiro/Downloads/agent_workspace/antisemitic-hate-crime-tracker-portable/website-source/node_modules/.pnpm/pnpm@10.12.1/node_modules:/Users/paulshapiro/Downloads/agent_workspace/antisemitic-hate-crime-tracker-portable/website-source/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/paulshapiro/Downloads/agent_workspace/antisemitic-hate-crime-tracker-portable/website-source/node_modules/.pnpm/pnpm@10.12.1/node_modules/pnpm/bin/node_modules:/Users/paulshapiro/Downloads/agent_workspace/antisemitic-hate-crime-tracker-portable/website-source/node_modules/.pnpm/pnpm@10.12.1/node_modules/pnpm/node_modules:/Users/paulshapiro/Downloads/agent_workspace/antisemitic-hate-crime-tracker-portable/website-source/node_modules/.pnpm/pnpm@10.12.1/node_modules:/Users/paulshapiro/Downloads/agent_workspace/antisemitic-hate-crime-tracker-portable/website-source/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pnpm/bin/pnpx.cjs" "$@"
else
  exec node  "$basedir/../pnpm/bin/pnpx.cjs" "$@"
fi
